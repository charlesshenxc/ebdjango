server {
    listen      9000; 

    root       /home/local/ANT/xuchao/plans/www;
    access_log /home/local/ANT/xuchao/plans/log/access_log;
    error_log  /home/local/ANT/xuchao/plans/log/error_log;

    location /favicon.ico {
        root /home/local/ANT/xuchao/plans/www;
    }

    location ~ ^\/static\/.*$ {
        root /home/local/ANT/xuchao/plans/www;
    }

    location / {
        #uwsgi_pass      127.0.0.1:9001;
        uwsgi_pass      unix:///tmp/itplans.socket;
        include         uwsgi_params;
        uwsgi_param     UWSGI_SCHEME $scheme;
        uwsgi_param     SERVER_SOFTWARE    nginx/$nginx_version;
    }
}
